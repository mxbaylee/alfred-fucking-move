				<key>script</key>
				<string>on run argv
    set theQuery to item 1 of argv
    
    if not running of application "Alfred" then return
    
    tell application "System Events"
        tell application process "Alfred"
            set |position| to position of window 1
            
            if theQuery is "alfred_up" then
                set position of window 1 to {((item 1 of |position|)), ((item 2 of |position|) - 200)}
            else if theQuery is "alfred_down" then
                set position of window 1 to {((item 1 of |position|)), ((item 2 of |position|) + 200)}
            else if theQuery is "alfred_left" then
                set position of window 1 to {((item 1 of |position|) - 200), ((item 2 of |position|))}
            else if theQuery is "alfred_right" then
                set position of window 1 to {((item 1 of |position|) + 200), ((item 2 of |position|))}
            end if
        end tell
    end tell
    
    return theQuery
end run</string>
